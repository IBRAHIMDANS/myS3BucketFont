{
  "name": "my-s3-bucket-front",
  "version": "1.0.0",
  "description": "Simple app as Amazon S3 with angular deploy to heroku",
  "author": "Ibrahima DANSOKO <ibrahima.dansoko@outlook.com>, Benjamin Benoit",
  "scripts": {
    "ng": "ng",
    "start:heroku": "node mainServer.js",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "postinstall": "ng build --aot --prod",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run myS3BucketFront:server:production --bundleDependencies all"
  },
  "private": true,
  "engines": {
    "node": ">=12.7.x",
    "npm": ">=6.13.4",
    "yarn": ">=1.19.x"
  },
